<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:clr="clr-namespace:System;assembly=mscorlib"
                    xmlns:ConRsCoverter="clr-namespace:LCPReportingSystem.RsConverter">

    <!--#region Global String-->

    <clr:String x:Key="StrMainAnnouncement">This subsystem configuration information can be edited by the 
            user with a click of the edit button. All editable information will be reflected after clicking the refresh button.</clr:String>

    <clr:String x:Key="ConfigAnnouncementText"> Sub-System Configuraton  Details</clr:String>
    <clr:String x:Key="LineGraphAnnouncement">Sub-System Graphical Data</clr:String>
    <clr:String x:Key="MngArciveAnnouncementText">Sub-System Archive Data </clr:String>

    <!--#endregion-->
    
    <!--#region Color Code -->
    
    <Color x:Key="Color2B0233">#2B0233</Color>              
    <Color x:Key="Color4F0533">#4F0533</Color>
    <Color x:Key="Color071830">#071830</Color>
    <Color x:Key="Color03284B">#03284B</Color>
    <Color x:Key="Color03163E">#03163E</Color>
    <Color x:Key="Color061E46">#061E46</Color>
    <Color x:Key="Color033D5E">#033D5E</Color>
    <Color x:Key="Color034B7C">#034B7C</Color>
    <Color x:Key="ColorF78103">#F78103</Color>
    <Color x:Key="ColorFC0037">#FC0037</Color>
    <Color x:Key="ColorF75314">#F75314</Color>
    <Color x:Key="ColorCD6155">#CD6155</Color>
    <Color x:Key="Color710000">#710000</Color>
    <Color x:Key="ColorC84603">#C84603</Color>
    <Color x:Key="ColorED2436">#ED2436</Color>

    <SolidColorBrush x:Key="Brush5B0D33" Color="#5B0D33"/>
    <SolidColorBrush x:Key="Brush03284B" Color="#03284B"/>
    <SolidColorBrush x:Key="Brush2E4D78" Color="#2E4D78"/>
    <SolidColorBrush x:Key="Brush00517D" Color="#00517D"/>
    <SolidColorBrush x:Key="Brush39525E" Color="#39525E"/>
    <SolidColorBrush x:Key="Brush061E46" Color="#061E46"/>
    <SolidColorBrush x:Key="BrushF78103" Color="#F78103"/>
    <SolidColorBrush x:Key="BrushFC0037" Color="#FC0037"/>
    <SolidColorBrush x:Key="BrushF75314" Color="#F75314"/>
    <SolidColorBrush x:Key="BrushCD6155" Color="#CD6155"/>
    <SolidColorBrush x:Key="Brush710000" Color="#710000"/>
    <SolidColorBrush x:Key="Brush033D5E" Color="#033D5E"/>
    <SolidColorBrush x:Key="BrushC84603" Color="#C84603"/>
    <SolidColorBrush x:Key="BrushED2436" Color="#ED2436"/>

    <LinearGradientBrush x:Key="GradientBackground" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="{StaticResource Color2B0233}" Offset="0"/>
        <GradientStop Color="{StaticResource Color4F0533}" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="GradientBackground2" StartPoint="3,0.25" EndPoint="250,350">
        <GradientStop Color="{StaticResource Color033D5E}"/>
        <GradientStop Color="{StaticResource Color034B7C}"/>
    </LinearGradientBrush>
    
    <LinearGradientBrush x:Key="GradientBackground3" StartPoint="3,0.25" EndPoint="250,350">
        <GradientStop Color="#CAE1E8"/>
        <GradientStop Color="#DCF5E8"/>
    </LinearGradientBrush>
    <!--#endregion-->

    <!--#region BitmapImage Images -->
    
    <BitmapImage x:Key="imgverify" UriSource="/Assarts/verify.png"/>
    <BitmapImage x:Key="imgverifySuper" UriSource="/Assarts/AddUser.png"/>
    <BitmapImage x:Key="rptBackImage" UriSource="/Assarts/Dashbord.jpg"/>
    
    <!--#endregion-->

    <!--#region Converter-->
    
    <ConRsCoverter:ColorIndicator x:Key="convertColer"/>
    <BooleanToVisibilityConverter x:Key="ConvertBoleanVisibility"/>
    
    <!--#endregion-->
    
</ResourceDictionary>