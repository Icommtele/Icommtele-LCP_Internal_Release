                        <Button.Effect>
                            <DropShadowEffect ShadowDepth="1" Color="#061E46"/>
                        </Button.Effect>
                    </Button>
                    <TextBlock Grid.Column="2" Grid.Row="2" Margin="32,0,0,0" Width="130" FontSize="16"  Grid.ColumnSpan="3" Text="{Binding Path=dashboard.UpsInputLineBadsTimestamp}" Style="{StaticResource TextBlockText}"/>

                    <!--#endregion

                                         #region upsInputVoltage-->
                    <materialdesign:PackIcon  Grid.Column="0" Grid.Row="3" Margin="0,0,0,0" Foreground="CadetBlue" Height="20"  Width="29" Kind="PowerSocketChina" Grid.ColumnSpan="2" />

                    <TextBlock Grid.Column="1" Grid.Row="3" Width="230" HorizontalAlignment="Left" Text="UPS Input Voltage" Style="{StaticResource TextBlockText}" Grid.ColumnSpan="2"/>
                    <TextBlock Grid.Column="2" Grid.Row="3" Height="20"  Width="130" Margin="70,5,0,5"    FontSize="16" Text="{Binding Path=dashboard.UpsInputVoltage}" />

                    <Button Grid.Column="5" Grid.Row="3" Margin="0,-2,0,0"  Height="17" Width="17" Background="#FF0000" Template="{StaticResource ctCircle}" Cursor="Hand"
                                                     BorderBrush="{Binding Path=dashboard.UpsInputVoltageIndicator,Converter={StaticResource convertColer}}">
                        <Button.Effect>
                            <DropShadowEffect ShadowDepth="1" Color="#061E46"/>
                        </Button.Effect>
                    </Button>
                    <TextBlock Grid.Column="3" Grid.Row="3" Margin="32 ,8, 0 ,0"  FontSize="16" Text="{Binding Path=dashboard.UpsInputVoltageTimestamp}" Style="{StaticResource ParametersValText}" Grid.ColumnSpan="3"/>

                    <!--#endregion

                                         #region upsInputCurrent-->
                    <materialdesign:PackIcon  Grid.Column="0" Grid.Row="4" Margin="0,0,0,0" Foreground="CadetBlue" Height="20"  Width="29" Kind="PowerSocketChina" Grid.ColumnSpan="2" />

                    <TextBlock Grid.Column="1" Grid.Row="4" Height="30"  Text="UPS Input Current" Style="{StaticResource TextBlockText}"/>
                    <TextBlock Grid.Column="2" Grid.Row="4" Height="20"   Margin="70,0,0,0" Text="{Binding Path=dashboard.UpsInputCurrent}"  FontSize="16"/>

                    <Button  Grid.Column="5" Grid.Row="4" Margin="0,-2,0,0"  Height="17" Width="17" Background="#FF0000" Template="{StaticResource ctCircle}" Cursor="Hand"
                                                     BorderBrush="{Binding Path=dashboard.UpsInputCurrentIndicator,Converter={StaticResource convertColer}}">
                        <Button.Effect>
                            <DropShadowEffect ShadowDepth="1" Color="#061E46"/>
                        </Button.Effect>
                    </Button>
                    <TextBlock  Grid.Column="3" Grid.Row="4" Margin="32 ,8, 0 ,0"  FontSize="16" Text="{Binding Path=dashboard.UpsInputCurrentTimestamp}" Style="{StaticResource ParametersValText}" Grid.ColumnSpan="3"/>

                    <!--#endregion

                                         #region upsInputTruePower-->
                    <materialdesign:PackIcon  Grid.Column="0" Grid.Row="5" Margin="0,0,0,0" Foreground="CadetBlue" Height="20"  Width="29" Kind="PowerSocketChina" Grid.ColumnSpan="2" />
                    <TextBlock  Grid.Column="1" Grid.Row="5" Margin="0,0,0,5"  Text="UPS Input True Power" Style="{StaticResource TextBlockText}"/>
                    <TextBlock  Height="20"  Grid.Column="2" Grid.Row="5" Margin="70,0,0,0"    Text="{Binding Path=dashboard.UpsInputTruePower}"  FontSize="16"/>
                    <Button Grid.Column="5" Grid.Row="5" Margin="0,-2,0,0"  Height="17" Width="17" Background="#FF0000" Template="{StaticResource ctCircle}" Cursor="Hand"
                                                     BorderBrush="{Binding Path=dashboard.UpsInputTruePowerIndicator,Converter={StaticResource convertColer}}">
                        <Button.Effect>
                            <DropShadowEffect ShadowDepth="1" Color="#061E46"/>
                        </Button.Effect>
                    </Button>
                    <TextBlock Grid.Column="3" Grid.Row="5" Margin="32 ,8, 0 ,0" FontSize="16" Text="{Binding Path=dashboard.UpsInputTruePowerTimestamp}" Style="{StaticResource ParametersValText}" Grid.ColumnSpan="3"/>
                </Grid>
            </Border>
        </Border>

