                    <TextBlock Grid.Column="3" Grid.Row="1" Margin="32 ,0, 0 ,0" FontSize="16" Text="{Binding Path=dashboard.UpsInputFrequencyTimestamp}" Style="{StaticResource ParametersValText}" Grid.ColumnSpan="3"/>

                    <!--#endregion
