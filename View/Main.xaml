                <SolidColorBrush Color="#0064C1"/>
            </StackPanel.Background>
            <StackPanel.Effect>
                <DropShadowEffect ShadowDepth="1"/>
            </StackPanel.Effect>
            <StackPanel>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="550"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Border Grid.Column="0" Height="50">
                        <StackPanel Orientation="Horizontal">
                            <Border Margin="-30,0,0,0" Height="50" Width="100" >
                                <Button Template="{StaticResource CT_SliderOn}">
                                    <Image Source="{StaticResource mainHederIcon}" Height="30" Width="30" Cursor="Hand">
                                    </Image>
                                    <Button.Triggers>
                                        <EventTrigger RoutedEvent="Button.Click">
                                            <BeginStoryboard>
                                                <Storyboard>
                                                    <DoubleAnimation Storyboard.TargetName="DashbordSlider" Storyboard.TargetProperty="Width" From="0" To="210" Duration="0:0:0.3"/>
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </EventTrigger>
                                    </Button.Triggers>
                                </Button>
                            </Border>
                            <Border Height="50" Margin="0 0 0 0">
                                <Border.BorderThickness>
                                    <Thickness Right="0.2"/>
                                </Border.BorderThickness>
                                <Border.BorderBrush>
                                    <SolidColorBrush Color="#EEEFF3"/>
                                </Border.BorderBrush>
                                <Image Source="{StaticResource IcommIcon}" Height="80" Width="60" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="-20,5,10,0" Cursor="Hand">
                                </Image>

                            </Border>
                            <!--<Border Margin="10 0 0 0">
                                <TextBlock Text="Welcome :" Style="{StaticResource txtMainHederCaption}"/>
                            </Border>-->
                            <Canvas x:Name="TextCanvas" ClipToBounds="True" Height="20"    Width="640" Background="Transparent">
                                <TextBlock x:Name="ScrollingText" Visibility="Visible"  Width="640"  HorizontalAlignment="Left"
                                         FontFamily="Arial"
                                         Text="LAUNCH COMMAND PROCESS REPORTING SYSTEM"   Margin="20,0,0,0"
                                         Foreground="White"
                                         FontSize="16"
                                         FontWeight="Bold"
                                         Background="Transparent" />
                            </Canvas>
                        </StackPanel>
                    </Border>
                    <Border Grid.Column="1" >
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                           
                            <TextBlock Margin="20 2 0 0"  Text="{Binding Path=dashboard.GreetingMessage}" Style="{StaticResource txtGreetingContent}"/>
                            <StackPanel Grid.Column="0" Grid.Row="0" Orientation="Horizontal" HorizontalAlignment="Right" Margin="10,0,0,0"  VerticalAlignment="Center">
                                <materialdesign:PackIcon Foreground="YellowGreen" Height="25" Width="25" Kind="TimerOutline" />
                            </StackPanel>
                            <!--<TextBlock Margin="0 0 0 0" Text="CURRENT DATE / TIME :" Style="{StaticResource txtMainHederCaption}"/>-->

                            <Border Margin="5 2 30 0">
                                <TextBlock Text="{Binding Path=dashboard.Currentdatetime}" Style="{StaticResource txtMainHederContent2}"/>
                            </Border>
                            <Border Height="50" Margin="0 0 0 0">
                                <Border.BorderThickness>
                                    <Thickness Right="0"/>
                                </Border.BorderThickness>
                                <Border.BorderBrush>
                                    <SolidColorBrush Color="#EEEFF3"/>
                                </Border.BorderBrush>
                                <StackPanel Grid.Column="0" Grid.Row="0" Orientation="Horizontal" HorizontalAlignment="Right" Margin="0,0,0,0"  VerticalAlignment="Center">
                                    <materialdesign:PackIcon Foreground="YellowGreen" Height="30" Width="30" Kind="UserCircle" />
                                </StackPanel>
                            </Border>
                            <Border Height="50" Margin="2 0 30 0">
                                <Border.BorderThickness>
                                    <Thickness Right="0.0"/>
                                </Border.BorderThickness>
                                <Border.BorderBrush>
                                    <SolidColorBrush Color="#EEEFF3"/>
                                </Border.BorderBrush>
                                <TextBlock Text="{Binding Path=dashboard.UserName}" Style="{StaticResource txtMainHederContent}" Margin="0, 0, 10, 0"/>

                            </Border>


                           
                            <Button x:Name="btn_Logout" Style="{StaticResource BigButton}"  CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Tag}" >
                                <StackPanel Orientation="Horizontal">
                                    <materialdesign:PackIcon Foreground="white" Height="20" Width="27" Background="YellowGreen" Kind="Logout" />
                                    <TextBlock VerticalAlignment="Center" Margin="5 0 0 0" Text="Logout" FontFamily="Arial Unicode MS Regular" FontSize="14">
                                    </TextBlock>
                                </StackPanel>
                            </Button>
                        </StackPanel>
                    </Border>
                </Grid>
            </StackPanel>
