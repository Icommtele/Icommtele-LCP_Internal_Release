            <TextBlock x:Name="txtNetworkStatus"  Margin="5,0,0,0" FontSize="16" FontWeight="SemiBold" Foreground="White"></TextBlock>

        </StackPanel>
        <!--#region Dashboard Header Start-->
        <StackPanel Height="50" VerticalAlignment="Top">
